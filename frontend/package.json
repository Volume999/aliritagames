    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@vitejs/plugin-react": "^4.3.3",
    "eslint": "^8",
    "eslint-config-next": "15.0.2",
    "jsdom": "^25.0.1",
    "lint-staged": "^15.2.10",
